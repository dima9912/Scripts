local id = game.Players.LocalPlayer.UserId
local namelocalplayera = game.Players.LocalPlayer.Name
ofdodldl = nil

for _, plot in pairs(workspace.Plots:GetChildren()) do
    if plot:IsA("Model") then
        local at = plot:GetAttribute("Owner")
        if at == id then
--юзать plot.Хуцня
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local nameforall = "Pizda"

local FolderCoreGuilmao = Instance.new("Folder")
FolderCoreGuilmao.Name = nameforall
FolderCoreGuilmao.Parent = CoreGui

local screenGui = Instance.new("ScreenGui")
screenGui.Name = nameforall
screenGui.Parent = FolderCoreGuilmao

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 270)
frame.Position = UDim2.new(0, 1380, 0, 0)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.Parent = screenGui
frame.Name = nameforall

local nickLabel = Instance.new("TextLabel")
nickLabel.Size = UDim2.new(1, 0, 1, 0)
nickLabel.Position = UDim2.new(0, 0, 0, -125) 
nickLabel.BackgroundTransparency = 1
nickLabel.Text = "Menu for steal"
nickLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
nickLabel.TextSize = 23
nickLabel.Font = Enum.Font.Gotham
nickLabel.Parent = frame
nickLabel.Name = nameforall

local uicornerforframe = Instance.new("UICorner")
uicornerforframe.CornerRadius = UDim.new(0, 30)
uicornerforframe.Parent = frame
uicornerforframe.Name = nameforall

local x = Instance.new("UIStroke")
x.Parent = frame
x.Thickness = 2
x.Color = Color3.fromRGB(255, 255, 255)
x.Name = nameforall

local button1 = Instance.new("TextButton")
button1.Parent = frame
button1.Size = UDim2.new(0, 130, 0, 50)
button1.Position = UDim2.new(0, 38, 0, 30)
button1.Text = "Тп на базу"
button1.Font = Enum.Font.Gotham
button1.TextSize = 15
button1.Name = nameforall

local didiblat = Instance.new("UICorner")
didiblat.Parent = button1

button1.MouseButton1Click:Connect(function()
Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plot.AnimalTarget.CFrame
end)

local button3 = Instance.new("TextButton")
button3.Parent = frame
button3.Size = UDim2.new(0, 130, 0, 50)
button3.Position = UDim2.new(0, 38, 0, 190)
button3.Text = "Быстрая активация"
button3.Font = Enum.Font.Gotham
button3.TextSize = 15
button3.Name = nameforall

local uuuusssi = Instance.new("UICorner")
uuuusssi.Parent = button3
uuuusssi.Name = nameforall

button3.MouseButton1Click:Connect(function()
	task.spawn(function()
		local success, err = pcall(function()
			local workspace = game:GetService("Workspace")
			
			local function makeInstant(prompt)
				if not prompt or not prompt:IsA("ProximityPrompt") then return end
				
				pcall(function() prompt.HoldDuration = 0 end)
				pcall(function() prompt.RequiresLineOfSight = false end)
				
				pcall(function()
					prompt:GetPropertyChangedSignal("HoldDuration"):Connect(function()
						task.spawn(function()
							task.wait(0.01)
							pcall(function() prompt.HoldDuration = 0 end)
						end)
					end)
				end)
			end

			for _, obj in workspace:GetDescendants() do
				if obj:IsA("ProximityPrompt") then
					task.spawn(makeInstant, obj)
				end
			end

			workspace.DescendantAdded:Connect(function(obj)
				if obj:IsA("ProximityPrompt") then
					task.wait()
					task.spawn(makeInstant, obj)
				end
			end)
		end)
	end)
end)

local button2 = Instance.new("TextButton")
button2.Parent = frame
button2.Size = UDim2.new(0, 130, 0, 50)
button2.Position = UDim2.new(0, 38, 0, 110)
button2.Text = "Авто-спиздилка"
button2.Font = Enum.Font.Gotham
button2.TextSize = 15
button2.Name = nameforall

local uuuui = Instance.new("UICorner")
uuuui.Parent = button2
uuuui.Name = nameforall

getgenv().autosteal = true

button2.MouseButton1Click:Connect(function()
while autosteal do
if Players.LocalPlayer:GetAttribute("Stealing") == true then
    ofdodldl = Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    task.wait(0.1)
	Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plot.AnimalTarget.CFrame
	task.wait()
	Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
	task.wait(0.2)
	Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ofdodldl
	Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end
task.wait(0.1)
end

end)
end
end
end
--script blyat

loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()

while game:GetService("RunService").RenderStepped:wait() do
	workspace:WaitForChild("Part"):Destroy()
end
