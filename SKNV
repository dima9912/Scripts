local function gg4()
-- Активация ProximityPrompt через локальный скрипт (работает только если античит не мониторит RemoteEvents)
local proximityPrompt = workspace:FindFirstChild("CTInteract").ProximityPrompt
if proximityPrompt then
    proximityPrompt:InputHoldBegin() -- Симулируем начало взаимодействия
    task.wait(0.5)
    proximityPrompt:InputHoldEnd() -- Завершаем действие
end
end

