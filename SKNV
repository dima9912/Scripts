-- Misc
local Misc = Window:NewTab("Всякое")
local PowerSection = Misc:NewSection("Игрок")

PowerSection:NewSlider("Скорость", "Скорость", 50, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PowerSection:NewSlider("Прыжок", "Прыжок", 200, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

local ServerSection = Misc:NewSection("Сервер")
ServerSection:NewButton("Перезайти (Скрипт нужно перезапустить)", "Перезайти на сервер (на этот, не на другой)", function()
    local ts = game:GetService("TeleportService")
    local p = game:GetService("Players").LocalPlayer

    ts:Teleport(game.PlaceId, p)
end)
