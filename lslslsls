button3.MouseButton1Click:Connect(function()
    task.spawn(function()
        local success, err = pcall(function()
            local workspace = game:GetService("Workspace")
            
            local function makeInstant(prompt)
                if not prompt or not prompt:IsA("ProximityPrompt") then return end
                
                pcall(function() prompt.HoldDuration = 0 end)
                pcall(function() prompt.RequiresLineOfSight = false end)
                
                pcall(function()
                    prompt:GetPropertyChangedSignal("HoldDuration"):Connect(function()
                        task.spawn(function()
                            task.wait(0.01)
                            pcall(function() prompt.HoldDuration = 0 end)
                        end)
                    end)
                end)
            end

            for _, obj in workspace:GetDescendants() do
                if obj:IsA("ProximityPrompt") then
                    task.spawn(makeInstant, obj)
                end
            end

            workspace.DescendantAdded:Connect(function(obj)
                if obj:IsA("ProximityPrompt") then
                    task.wait()
                    task.spawn(makeInstant, obj)
                end
            end)
        end)
    end)
end)

button1.MouseButton1Click:Connect(function()
Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plot.AnimalTarget.CFrame
end)

button2.MouseButton1Click:Connect(function()
    if autosteal == false then
	wait(0)
								StarterGui:SetCore("SendNotification", {
                        Title = "–ê–≤—Ç–æ-—Å–ø–∏–∑–¥–∏–ª–∫–∞",
                        Text = "–í–∫–ª—é—á–µ–Ω–∞",
                        Duration = 2,
                    })
        getgenv().autosteal = true
        while autosteal do
            if Players.LocalPlayer:GetAttribute("Stealing") == true then
                for i = 1, 20 do
                    game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Net"):WaitForChild("RE/dfdd4236-1fac-4b9a-9f67-fec68d72c151"):FireServer(unpack(args))
                end
            end
            task.wait(0.1)
        end
    else
	wait()
        getgenv().autosteal = false
									StarterGui:SetCore("SendNotification", {
                        Title = "–ê–≤—Ç–æ-—Å–ø–∏–∑–¥–∏–ª–∫–∞",
                        Text = "–û—Ç–∫–ª—é—á–µ–Ω–∞",
                        Duration = 2,
                    })
    end
end)

button4.MouseButton1Click:Connect(function()
plot.PlotSign.SurfaceGui.Frame.TextLabel:GetPropertyChangedSignal("Text"):Connect(protectnamebase)
game:GetService("CoreGui").PlayerList.Children.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScrollingFrame.OffsetUndoFrame.TeamList_Neutral:FindFirstChild("PlayerEntry_" .. game.Players.LocalPlayer.UserId).PlayerEntryContentFrame.OverlayFrame.NameFrame.PlayerName.PlayerName:GetPropertyChangedSignal("Text"):Connect(protectnameintab)
protectnamebase()
protectnameintab()	
end)

--Anims
local frameinf = TweenInfo.new(1.5, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out)
local frameinf2 = {Size = UDim2.new(0, 200, 0, 340)}
local frameanim = LSSOS:Create(frame, frameinf, frameinf2)
local buttonaimka = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local button1Anim = LSSOS:Create(button1, buttonaimka, {BackgroundTransparency = 0, TextTransparency = 0})
local button2Anim = LSSOS:Create(button2, buttonaimka, {BackgroundTransparency = 0, TextTransparency = 0})
local button3Anim = LSSOS:Create(button3, buttonaimka, {BackgroundTransparency = 0, TextTransparency = 0})
local button4Anim =  LSSOS:Create(button4, buttonaimka, {BackgroundTransparency = 0, TextTransparency = 0})
local textguianim = LSSOS:Create(TextLabel, buttonaimka, {TextTransparency = 0})

frameanim:Play()
task.wait(1.5)

task.wait(0.2)
textguianim:Play()
task.wait(0.1)
button1.TextColor3 = Color3.fromRGB(0, 0, 0)
button1Anim:Play()
task.wait(0.1)
button2.TextColor3 = Color3.fromRGB(0, 0, 0)
button2Anim:Play()
task.wait(0.1)
button3.TextColor3 = Color3.fromRGB(0, 0, 0)
button3Anim:Play()
task.wait(0.1)
button4.TextColor3 = Color3.fromRGB(0, 0, 0)
button4Anim:Play()

task.wait(0.2)
soundddd:Play()

StarterGui:SetCore("SendNotification", {
    Title = "–ú–µ–Ω—é –£–†–ë",
    Text = "–ì—É–∏ –∑–∞–≥—Ä—É–∂–µ–Ω–æ",
    Duration = 5,
})

