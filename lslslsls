--Functions
local function protectnamebase() 
    plot.PlotSign.SurfaceGui.Frame.TextLabel.Text = "–ê —Ö—É–π —Ç–∞–º –ø–ª–∞–≤–∞–ª"
end

local function protectnameintab()
game:GetService("CoreGui").PlayerList.Children.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScrollingFrame.OffsetUndoFrame.TeamList_Neutral:FindFirstChild("PlayerEntry_" .. game.Players.LocalPlayer.UserId).PlayerEntryContentFrame.OverlayFrame.NameFrame.PlayerName.PlayerName.Text = "–ê —Ö—É–π —Ç–∞–º –ø–ª–∞–≤–∞–ª"
end

